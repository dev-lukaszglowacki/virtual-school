{
  "realm": "virtual-school",
  "enabled": true,
  "clients": [
    {
      "clientId": "virtual-school-ui",
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*",
        "http://127.0.0.1:3000/*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://127.0.0.1:3000"
      ],
      "directAccessGrantsEnabled": true,
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "realm_access.roles",
            "jsonType.label": "String",
            "user.attribute": "foo",
            "id.token.claim": "false",
            "access.token.claim": "true",
            "multivalued": "true"
          }
        }
      ]
    },
    {
      "clientId": "admin-cli",
      "secret": "my-secret",
      "protocol": "openid-connect",
      "publicClient": false,
      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": true
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role",
        "composite": false,
        "clientRole": false,
        "containerId": "virtual-school"
      },
      {
        "name": "teacher",
        "description": "Teacher role",
        "composite": false,
        "clientRole": false,
        "containerId": "virtual-school"
      },
      {
        "name": "student",
        "description": "Student role",
        "composite": false,
        "clientRole": false,
        "containerId": "virtual-school"
      }
    ]
  },
  "users": [
    {
      "username": "adminuser",
      "enabled": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin.user@example.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "adminpass"
        }
      ],
      "realmRoles" : [ "default-roles-virtual-school", "offline_access", "uma_authorization", "admin" ],
      "clientRoles": {
        "realm-management": [
          "realm-admin"
        ]
      }
    },
    {
      "username": "teacheruser",
      "enabled": true,
      "firstName": "Teacher",
      "lastName": "User",
      "email": "teacher.user@example.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "teacherpass"
        }
      ],
      "realmRoles": [
        "teacher"
      ]
    },
    {
      "username": "studentuser",
      "enabled": true,
      "firstName": "Student",
      "lastName": "User",
      "email": "student.user@example.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "studentpass"
        }
      ],
      "realmRoles": [
        "student"
      ]
    }
  ]
}
